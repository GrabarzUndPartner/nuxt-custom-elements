{
  "name": "nuxt-custom-elements",
  "version": "1.2.2",
  "description": "Publish your Components as a vue-custom-element standalone build.",
  "keywords": [
    "nuxtjs",
    "vuejs",
    "vue-custom-element",
    "custom-element",
    "shadow-dom"
  ],
  "repository": "GrabarzUndPartner/nuxt-custom-elements",
  "license": "MIT",
  "author": "Thorn-Welf Walli",
  "contributors": [
    {
      "name": "Thorn-Welf Walli",
      "email": "thorn.walli@grabarzundpartner.de"
    }
  ],
  "files": [
    "lib"
  ],
  "main": "lib/module.js",
  "scripts": {
    "dev": "nuxt example",
    "generate": "nuxt generate --config-file example/nuxt.config.js",
    "build": "nuxt build --config-file example/nuxt.config.js",
    "start:build": "yarn build && yarn express-server",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint --ext .js,.vue example lib test",
    "lint:css": "stylelint \"example/**/*.vue\" \"example/**/*.css\", \"lib/**/*.vue\" \"lib/**/*.css\"",
    "express-server": "node ./tools/express-server.js --dist dist",
    "test": "yarn lint && jest"
  },
  "dependencies": {
    "change-case": "4.1.1",
    "document-register-element": "1.14.3",
    "vue-custom-element": "3.2.14"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@nuxt/builder": "2.13.0",
    "@nuxt/config": "2.13.0",
    "@nuxt/core": "2.13.0",
    "@nuxt/webpack": "2.13.0",
    "@nuxtjs/eslint-config": "3.0.0",
    "@nuxtjs/module-test-utils": "1.6.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "codecov": "3.7.0",
    "eslint": "7.3.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "nuxt-edge": "latest",
    "standard-version": "8.0.0",
    "stylelint": "13.6.1",
    "stylelint-config-recess-order": "2.0.4",
    "stylelint-config-standard": "20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
