{
  "name": "nuxt-custom-elements",
  "version": "1.5.0",
  "description": "Publish your Components as a vue-custom-element standalone build.",
  "keywords": [
    "nuxtjs",
    "vuejs",
    "vue-custom-element",
    "custom-element",
    "shadow-dom"
  ],
  "repository": "GrabarzUndPartner/nuxt-custom-elements",
  "license": "MIT",
  "author": "Thorn-Welf Walli",
  "contributors": [
    {
      "name": "Thorn-Welf Walli",
      "email": "thorn.walli@grabarzundpartner.de"
    }
  ],
  "files": [
    "lib"
  ],
  "main": "lib/module.js",
  "scripts": {
    "dev": "nuxt example",
    "generate": "nuxt generate --config-file example/nuxt.config.js",
    "build": "nuxt build --config-file example/nuxt.config.js",
    "start:build": "npm run build && npm run express-server",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue example lib test",
    "lint:css": "stylelint \"example/**/*.vue\" \"example/**/*.css\", \"lib/**/*.vue\" \"lib/**/*.css\"",
    "express-server": "node ./tools/express-server.js --dist dist",
    "test": "npm run lint && jest",
    "jest": "jest"
  },
  "dependencies": {
    "@ungap/custom-elements": "0.1.15",
    "change-case": "4.1.2",
    "vue-custom-element": "3.2.14"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/preset-env": "7.12.16",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "express": "4.17.1",
    "@nuxt/builder": "2.15.0",
    "@nuxt/config": "2.15.0",
    "@nuxt/core": "2.15.0",
    "@nuxt/webpack": "2.15.0",
    "@nuxtjs/eslint-config": "5.0.0",
    "@nuxtjs/module-test-utils": "1.6.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "codecov": "3.8.1",
    "core-js": "3.8.3",
    "eslint": "7.20.0",
    "husky": "5.0.9",
    "jest": "26.6.3",
    "nuxt-edge": "latest",
    "puppeteer": "7.1.0",
    "standard-version": "9.1.1",
    "stylelint": "13.10.0",
    "stylelint-config-recess-order": "2.3.0",
    "stylelint-config-standard": "20.0.0",
    "vue-i18n": "8.22.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
