{
  "name": "nuxt-custom-elements",
  "version": "1.3.3",
  "description": "Publish your Components as a vue-custom-element standalone build.",
  "keywords": [
    "nuxtjs",
    "vuejs",
    "vue-custom-element",
    "custom-element",
    "shadow-dom"
  ],
  "repository": "GrabarzUndPartner/nuxt-custom-elements",
  "license": "MIT",
  "author": "Thorn-Welf Walli",
  "contributors": [
    {
      "name": "Thorn-Welf Walli",
      "email": "thorn.walli@grabarzundpartner.de"
    }
  ],
  "files": [
    "lib"
  ],
  "main": "lib/module.js",
  "scripts": {
    "dev": "nuxt example",
    "generate": "nuxt generate --config-file example/nuxt.config.js",
    "build": "nuxt build --config-file example/nuxt.config.js",
    "start:build": "yarn build && yarn express-server",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint --ext .js,.vue example lib test",
    "lint:css": "stylelint \"example/**/*.vue\" \"example/**/*.css\", \"lib/**/*.vue\" \"lib/**/*.css\"",
    "express-server": "node ./tools/express-server.js --dist dist",
    "test": "yarn lint && jest",
    "jest": "jest"
  },
  "dependencies": {
    "change-case": "4.1.1",
    "document-register-element": "1.14.10",
    "vue-custom-element": "3.2.14"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "core-js": "3.6.5",
    "@nuxt/builder": "2.14.7",
    "@nuxt/config": "2.14.7",
    "@nuxt/core": "2.14.7",
    "@nuxt/webpack": "2.14.7",
    "@nuxtjs/eslint-config": "4.0.0",
    "@nuxtjs/module-test-utils": "1.6.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.1",
    "codecov": "3.8.0",
    "eslint": "7.12.0",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "nuxt-edge": "latest",
    "standard-version": "9.0.0",
    "stylelint": "13.7.2",
    "stylelint-config-recess-order": "2.3.0",
    "stylelint-config-standard": "20.0.0",
    "puppeteer": "5.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
