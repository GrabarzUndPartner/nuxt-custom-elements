{
  "name": "nuxt-custom-elements",
  "version": "2.0.0-beta.30",
  "description": "Publish your Components as a vue-custom-element standalone build.",
  "keywords": [
    "nuxtjs",
    "vuejs",
    "vue-custom-element",
    "custom-element",
    "shadow-dom"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GrabarzUndPartner/nuxt-custom-elements"
  },
  "license": "MIT",
  "author": "Thorn-Welf Walli",
  "contributors": [
    {
      "name": "Thorn-Welf Walli",
      "email": "lammpee@gmail.de"
    }
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "type": "module",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "nuxt build playground",
    "dev": "nuxt dev playground",
    "generate": "nuxt generate playground",
    "preview": "nuxt preview",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:es": "eslint --ext .mjs,.js,.vue .",
    "lint:css": "stylelint \"playground/**/*.vue\" \"playground/**/*.css\", \"lib/**/*.vue\" \"lib/**/*.css\"",
    "lint:report": "eslint --ext .js,.vue -f json-relative -o ./eslint-report.json .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "start:static": "node ./tools/static-server.js --dist playground/.nuxt/nuxt-custom-elements/dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@unplugin-vue-ce/sub-style": "1.0.0-beta.19",
    "change-case": "5.4.2",
    "clone": "2.1.2",
    "defu": "6.1.4",
    "html-webpack-plugin": "5.6.0",
    "lodash-es": "4.17.21",
    "vue-web-component-wrapper": "1.3.4",
    "webpack-bundle-analyzer": "4.10.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.23.3",
    "@commitlint/cli": "18.5.0",
    "@commitlint/config-conventional": "18.5.0",
    "@nuxt/module-builder": "0.5.5",
    "@nuxt/webpack-builder": "3.9.3",
    "@nuxtjs/eslint-config": "12.0.0",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@types/node": "20.11.5",
    "commitlint": "18.5.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-formatter-json-relative": "0.1.0",
    "eslint-plugin-jsonc": "2.12.2",
    "eslint-plugin-no-unsanitized": "4.0.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-security": "2.1.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-xss": "0.1.12",
    "finalhandler": "1.2.0",
    "get-port-please": "3.1.2",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "markdown-it-inline-comments": "1.0.1",
    "nuxt": "3.9.3",
    "playwright": "1.41.1",
    "postcss-html": "1.6.0",
    "postcss-preset-env": "9.3.0",
    "prettier": "3.2.4",
    "read-pkg": "9.0.1",
    "serve-static": "1.15.0",
    "standard-version": "9.5.0",
    "stylelint": "16.2.0",
    "stylelint-config-recess-order": "4.4.0",
    "stylelint-config-standard": "36.0.0",
    "vitepress": "1.0.0-rc.40",
    "vitest": "1.2.1"
  }
}